// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model User {
  firstName String
  lastName  String?
  birthDate DateTime
  idType    String
  name      String?
  id        String @id
  salary    Int
}